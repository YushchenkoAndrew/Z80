CTRLR           	EQU 0x12             ; Control "R"
CTRLS           	EQU 0x13             ; Control "S"
CTRLU           	EQU 0x15             ; Control "U"
ESC             	EQU 0x1B             ; Escape
DEL             	EQU 0x7F             ; Delete
DELLN            	EQU 0x7D             ; Delete Line
NULL	           	EQU 0x00             ; NULL CHAR
BKSL	          	EQU 0x5C             ; Backslash
UPARR	          	EQU 0x5E             ; UP-Arrow




