KEYBOARD_INIT: EQU 0x03
UPSTROKE: EQU 0xF0

setup:
	LD SP, STACK      ; Set Memory Paging RAM
	EI                ; Enable interrupts
	IM 1              ; Use interrupt Mode 1

  ; Reset LCD
	LD A, 0x00
	OUT (0x30), A

  ; Create a small delay before start
